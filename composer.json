{
    "name": "robinmarechal/database-rest-unwrapper",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "RobinMarechal",
        "DatabaseRestUnwrapper"
    ],
    "homepage": "https://github.com/RobinMarechal/database-rest-unwrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Marechal",
            "email": "robin-marechal@hotmail.fr",
            "homepage": "http://github.com/RobinMarechal",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=7.0.0",
        "illuminate/database": "5.5.*",
        "illuminate/http": "5.5.*",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "RobinMarechal\\DatabaseRestUnwrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RobinMarechal\\DatabaseRestUnwrapper\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
